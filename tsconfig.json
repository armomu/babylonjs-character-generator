{
  "extends": "@vue/tsconfig/tsconfig.json",
  "include": ["src/**/*", "src/**/*.vue", "typings/*", "vite.config.*",],
  "compilerOptions": {
    "module": "ESNext",
    "baseUrl": ".",
    // "allowJs": true,
    "isolatedModules": false,
    "ignoreDeprecations": "5.0",
    "types": ["vite/client"],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
}
